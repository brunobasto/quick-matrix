{
  "compilerOptions": {
    "allowJs": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "build",
    "resolveJsonModule": true,
    "strict": true,
    "target": "es2018",
  },
  "include": ["src", "test/benchmark.ts"]
}